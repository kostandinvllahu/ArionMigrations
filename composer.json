{
    "name": "arionmigrations/arion-migrations",
    "description": "Laravel package for specifying column positions in migrations",
    "type": "library",
    "keywords": ["laravel", "migration", "column position"],
    "homepage": "https://github.com/kostandinvllahu/ArionMigrations",
    "license": "MIT",
    "authors": [
        {
            "name": "Kostandin Vllahu",
            "email": "kostandinvllahuatd@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.4",
        "illuminate/support": "^8.0 || ^8.83",  // Updated version constraint
    },
    "autoload": {
        "psr-4": {
            "ArionMigrations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArionMigrations\\Providers\\ArionMigrationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
